{"ast":null,"code":"import { getProList } from '@/api/product';\nimport GoodsItem from '@/components/GoodsItem.vue';\nexport default {\n  name: 'SearchIndex',\n  data() {\n    return {\n      page: 1,\n      proList: []\n    };\n  },\n  components: {\n    GoodsItem\n  },\n  computed: {\n    querySearch() {\n      return this.$route.query.search;\n    }\n  },\n  async created() {\n    const {\n      data: {\n        list\n      }\n    } = await getProList({\n      categoryId: this.$route.query.categoryId,\n      goodsName: this.querySearch,\n      page: this.page\n    });\n    this.proList = list.data;\n  }\n};","map":{"version":3,"names":["getProList","GoodsItem","name","data","page","proList","components","computed","querySearch","$route","query","search","created","list","categoryId","goodsName"],"sources":["src/views/search/list.vue"],"sourcesContent":["<template>\r\n  <div class=\"search\">\r\n    <van-nav-bar fixed title=\"商品列表\" left-arrow @click-left=\"$router.go(-1)\" />\r\n\r\n    <van-search\r\n      readonly\r\n      shape=\"round\"\r\n      background=\"#ffffff\"\r\n      :value=\"querySearch || '搜索商品'\"\r\n      show-action\r\n      @click=\"$router.push('/search')\"\r\n    >\r\n      <template #action>\r\n        <van-icon class=\"tool\" name=\"apps-o\" />\r\n      </template>\r\n    </van-search>\r\n\r\n    <!-- 排序选项按钮 -->\r\n    <div class=\"sort-btns\">\r\n      <div class=\"sort-item\">综合</div>\r\n      <div class=\"sort-item\">销量</div>\r\n      <div class=\"sort-item\">价格 </div>\r\n    </div>\r\n\r\n    <div class=\"goods-list\">\r\n      <GoodsItem v-for=\"item in proList\" :key=\"item.goods_id\" :item=\"item\"></GoodsItem>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getProList } from '@/api/product'\r\nimport GoodsItem from '@/components/GoodsItem.vue'\r\nexport default {\r\n  name: 'SearchIndex',\r\n  data () {\r\n    return {\r\n      page: 1,\r\n      proList: []\r\n    }\r\n  },\r\n  components: {\r\n    GoodsItem\r\