{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getHistory, setHistory } from '@/utils/storage';\nexport default {\n  name: 'SearchIndex',\n  data() {\n    return {\n      history: getHistory(),\n      search: '' // 输入框的内容\n    };\n  },\n  methods: {\n    goSearch(key) {\n      if (this.history.includes(key)) {\n        this.history = this.history.filter(item => item !== key);\n      }\n      this.history.unshift(key);\n      setHistory(this.history);\n      this.$router.push(`/searchlist?search=${key}`);\n    },\n    clearHistory() {\n      this.history = [];\n      setHistory([]);\n    }\n  }\n};","map":{"version":3,"names":["getHistory","setHistory","name","data","history","search","methods","goSearch","key","includes","filter","item","unshift","$router","push","clearHistory"],"sources":["src/views/search/index.vue"],"sourcesContent":["<template>\r\n  <div class=\"search\">\r\n    <van-nav-bar title=\"商品搜索\" left-arrow @click-left=\"$router.go(-1)\" />\r\n\r\n    <van-search v-model=\"search\" show-action placeholder=\"请输入搜索关键词\" clearable>\r\n      <template #action>\r\n        <div @click=\"goSearch(search)\">搜索</div>\r\n      </template>\r\n    </van-search>\r\n\r\n    <!-- 搜索历史 -->\r\n    <div class=\"search-history\" v-if=\"history.length>0\">\r\n      <div class=\"title\">\r\n        <span>最近搜索</span>\r\n        <van-icon @click=\"clearHistory\" name=\"delete-o\" size=\"16\" style=\"cursor:pointer\"/>\r\n      </div>\r\n      <div class=\"list\">\r\n        <div class=\"list-item\" @click=\"goSearch(item)\" v-for=\"item in history\" :key=\"item\" >{{item}}</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getHistory, setHistory } from '@/utils/storage'\r\nexport default {\r\n  name: 'SearchIndex',\r\n  data () {\r\n    return {\r\n      history: getHistory(),\r\n      search: '' // 输入框的内容\r\n    }\r\n  },\r\n  methods: {\r\n    goSearch (key) {\r\n      if (this.history.includes(key)) {\r\n